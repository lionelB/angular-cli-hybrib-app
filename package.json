{
  "name": "angular-phonecat-hybrid",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prei18n-extract-ajs": "tsc src/app-ajs/**/*.ts --outdir temp-i18n",
    "i18n-extract-ajs": "angular-gettext-cli --files '{temp-i18n/**/*.js,src/app-ajs/**/*.html}' --exclude '**/*.spec.js' --dest 'src/assets/i18n/extracted.pot'",
    "posti18n-extract-ajs": "del ./temp-i18n",
    "i18n-extract-app": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/extracted.pot --sort --format pot",
    "i18n-extract": "npm run i18n-extract-ajs && npm run i18n-extract-app",
    "i18n-compile": "find src -name *.po | cut -f1 -d. | xargs -I '{}' angular-gettext-cli --compile --files '{}'.po --dest '{}'.json --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/upgrade": "^4.1.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/node": "~6.0.60",
    "angular": "^1.5.11",
    "angular-animate": "^1.5.11",
    "angular-gettext": "^2.3.10",
    "angular-resource": "^1.5.11",
    "angular-route": "^1.5.11",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "createjs-module": "^0.8.3",
    "jquery": "^2.2.4",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/angular": "^1.6.17",
    "@types/angular-animate": "^1.5.6",
    "@types/angular-cookies": "^1.4.3",
    "@types/angular-mocks": "^1.5.9",
    "@types/angular-resource": "^1.5.8",
    "@types/angular-route": "^1.3.3",
    "@types/angular-sanitize": "^1.3.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "angular-gettext-cli": "^1.2.0",
    "codelyzer": "~3.2.0",
    "del-cli": "^1.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
